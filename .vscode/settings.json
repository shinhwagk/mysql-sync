{
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.wordWrap": "on",
  "python.defaultInterpreterPath": "/usr/bin/python3.11",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    },
  },
  "isort.args": [
    "--profile",
    "black"
  ],
}